    <script>
        document.addEventListener("DOMContentLoaded", function() {
        var nextButton = document.querySelector('.carousel-control-next');
        
        nextButton.addEventListener("click", function() {
            // Aktif olan carousel-item'ı bul
            var activeItem = document.querySelector('.carousel-item.active');
            // Aktif olan carousel-item'dan active sınıfını kaldır
            activeItem.classList.remove('active');
            // Bir sonraki karousel öğesini bul ve active sınıfını ekle
            var nextItem = activeItem.nextElementSibling;
            if (nextItem) {
                nextItem.classList.add('active');
            } else {
                // Son öğe ise ilk öğeye dön
                document.querySelector('.carousel-item:first-child').classList.add('active');
            }
        });
    });
</script>

